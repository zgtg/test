<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <!--邮件-->
  <servlet>
    <servlet-name>email</servlet-name>
    <servlet-class>com.yg.email.EmailServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>email</servlet-name>
    <url-pattern>/email/list</url-pattern>
  </servlet-mapping>

</web-app>
